<?xml version="1.0" standalone="no"?>
<svg xmlns:xlink="http://www.w3.org/1999/xlink" id="main-logo" width="400" height="120" viewBox="0 0 400 120" fill="none" xmlns="http://www.w3.org/2000/svg">
                    <!-- Symbol: Abstract 'G' connected to nodes -->
                    <g transform="translate(10, 10)">
                        <!-- The 'G' Arc representing the city -->
                        <path d="M70 20C42.3858 20 20 42.3858 20 70C20 97.6142 42.3858 120 70 120H90" stroke="#008751" stroke-width="12" stroke-linecap="round"/>
                        
                        <!-- Connection Nodes -->
                        <circle cx="90" cy="120" r="8" fill="#008751"/>
                        <circle cx="70" cy="20" r="8" fill="#008751"/>
                        
                        <!-- The "Connect" Tech Lines -->
                        <path d="M90 120L110 90L130 110" stroke="#000000" stroke-width="8" stroke-linecap="round" stroke-linejoin="round" class="text-secondary"/>
                        <circle cx="130" cy="110" r="6" fill="#000000" class="fill-secondary"/>
                        <circle cx="110" cy="90" r="6" fill="#000000" class="fill-secondary"/>
                        
                        <!-- Central Pulse/Location Dot -->
                        <circle cx="70" cy="70" r="10" fill="#008751">
                            <animate attributeName="r" values="8;12;8" dur="2s" repeatCount="indefinite"/>
                            <animate attributeName="opacity" values="1;0.7;1" dur="2s" repeatCount="indefinite"/>
                        </circle>
                    </g>

                    <!-- Text -->
                    <g transform="translate(160, 85)">
                        <text font-family="Montserrat" font-weight="900" font-size="52" fill="#008751" letter-spacing="-1">GIDI</text>
                        <text x="125" font-family="Montserrat" font-weight="400" font-size="52" fill="#1F2937" class="fill-text" letter-spacing="2">CONNECT</text>
                    </g>
                </svg>